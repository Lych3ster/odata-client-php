{
    "name": "dcis/odata-client",
    "description": "DCIS OData Client for PHP based on saintsystems",
    "keywords": [
        "odata",
        "rest",
        "php"
    ],
    "homepage": "https://github.com/Lych3ster/odata-client-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "DCIS",
            "email": "support_it@dudelange.lu"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.0 || ^3.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.5"
    },
    "autoload": {
        "files": [
            "src/Core/helpers.php"
        ],
        "psr-4": {
            "DCIS\\OData\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
